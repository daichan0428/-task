<h1 class="tasks_title">タスク一覧</h1>

  <tbody>
    <% @tasks.each do |task| %>
      <ul>
        <li><%= link_to task.title, task %>|<%= link_to '削除', task, method: :delete, data: { confirm: '削除OK？' } %></li>
      </ul>
    <% end %>
  </tbody>


<h2 class="tasks_new">タスク登録</h2>
<%= render 'form', task: @task %>
